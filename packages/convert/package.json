{
  "name": "@html2jsx/convert",
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "ISC",
  "keywords": [],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": "./*"
  },

  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",

  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
